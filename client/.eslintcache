odies/actions.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Actions/FetchHoodies/FetchHoodies.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Reducers/FetchJeans/FetchJeans.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Actions/FetchJeans/actions.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Actions/FetchJeans/FetchJeans.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Reducers/FetchSneakers/FetchSneakers.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Actions/FetchSneakers/action.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Actions/FetchSneakers/FetchSneakers.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/Components/Products/Hoodies.jsx",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/Components/Products/Jeans.jsx",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/Components/Products/Sneakers.jsx",["107"],"import React, { useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { FetchSneakers } from '../../store/Actions/FetchSneakers/FetchSneakers';\nimport { FetchProductByID } from '../../store/Actions/FetchProducByID/FetchProductByID';\nimport { useHistory } from 'react-router-dom';\nimport '../../styles/Products.scss';\n\nexport const Sneakers = () => {\n  const dispatch = useDispatch();\n  const { loading, error, sneakersList } = useSelector(\n    (state) => state.sneakers\n  );\n  const history = useHistory();\n  useEffect(() => {\n    dispatch(FetchSneakers());\n  }, [dispatch]);\n\n  const redirectToProductDetailsPage = (ID) => {\n    // dispatch(FetchProductByID(ID));\n    history.push(`/product-detail/${ID}`);\n  };\n\n  return (\n    <div className=\"Products\">\n      <div className=\"Products__header\">\n        <h1 className=\"products__category\">Sneakers</h1>\n      </div>\n      {loading ? (\n        <h4 className=\"Loader\">Fetching all Sneakers...</h4>\n      ) : error ? (\n        <h4>Network Error Fail to Fetch Jeans :'(</h4>\n      ) : (\n        <div className=\"Product__details\">\n          {sneakersList && sneakersList.products && (\n            <div className=\"details__container\">\n              {sneakersList.products.map((product) => {\n                return (\n                  <div className=\"details\" key={product.id}>\n                    <div className=\"details__image\">\n                      <img\n                        src={product.images[0].src}\n                        alt=\"\"\n                        onClick={() => redirectToProductDetailsPage(product.id)}\n                      />\n                    </div>\n                    <div className=\"details__description\">\n                      <h4>{product.handle}</h4>\n                      <h4>{`Price: R${product.variants[0].price}`}</h4>\n                    </div>\n                  </div>\n                );\n              })}\n            </div>\n          )}\n        </div>\n      )}\n    </div>\n  );\n};\n","/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Reducers/FetchProductByID/FetchProductByID.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Actions/FetchProducByID/actions.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/store/Actions/FetchProducByID/FetchProductByID.js",[],"/home/junior/SIDE-PROJECTS/Shopify-Store/client/src/Components/Products/ProductDetails.jsx",["108"],"import React, { useEffect, useState } from 'react';\nimport Client from 'shopify-buy';\nimport { useParams } from 'react-router-dom';\nimport '../../styles/ProductDetails.scss';\n\nconst client = Client.buildClient({\n  domain: process.env.REACT_APP_ShopifyDomain,\n  storefrontAccessToken: process.env.REACT_APP_StoreAccessToken\n});\n\nexport const ProductDetails = () => {\n  const { id } = useParams();\n  const [product, setProduct] = useState(null);\n  const [Qty, setQty] = useState(1);\n\n  const AddToCart = (variantID, qty) => {\n    const checkoutID = localStorage.getItem('checkout');\n    const lineItemsToAdd = [\n      {\n        variantId: variantID,\n        quantity: parseInt(qty)\n      }\n    ];\n\n    // Add an item to the checkout\n    client.checkout\n      .addLineItems(checkoutID, lineItemsToAdd)\n      .then((checkout) => {\n        // Do something with the updated checkout\n        console.log(checkout.lineItems); // Array with one additional line item\n      });\n  };\n\n  useEffect(() => {\n    client.product\n      .fetch(id)\n      .then((product) => {\n        setProduct(product);\n        // alert(JSON.stringify(product.images[0]));\n      })\n      .catch((error) => {\n        console.error(error);\n      });\n  }, [product]);\n\n  if (product === null) {\n    return <h2>Fetching product details...</h2>;\n  } else {\n    return (\n      <div className=\"singleProduct__details\">\n        {product && (\n          <div className=\"details__left\">\n            <div className=\"left__image\">\n              <img src={product.images[0].src} alt=\"\" />\n            </div>\n            <div className=\"left__handle\">\n              <h4>{product.handle}</h4>\n            </div>\n          </div>\n        )}\n        {product && (\n          <div className=\"details__right\">\n            <h3>Product Summary</h3>\n            <h4>{`Total Price: R${product.variants[0].price}`}</h4>\n            Qty\n            <select value={Qty} onChange={(e) => setQty(e.target.value)}>\n              <option value={1}>1</option>\n              <option value={2}>2</option>\n              <option value={3}>3</option>\n              <option value={4}>4</option>\n            </select>\n            <button\n              className=\"btn-add-to-cart\"\n              onClick={() => AddToCart(product.variants[0].id, Qty)}\n            >\n              Add To Cart\n            </button>\n          </div>\n        )}\n      </div>\n    );\n  }\n};\n","/home/junior/SIDE-PROJECTS/Shopify-St